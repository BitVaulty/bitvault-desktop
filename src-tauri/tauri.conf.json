{
  "productName": "bitvaulty",
  "version": "0.1.0",
  "identifier": "com.bitvaulty.app",
  "build": {
    "beforeDevCommand": "trunk serve --port=1776",
    "devUrl": "http://localhost:1776",
    "beforeBuildCommand": "trunk build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "BitVaulty",
        "width": 1920,
        "height": 1080
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
